!!! 5
%html
  %head
    %title Bracket
    = stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true
    = javascript_include_tag "application", "data-turbolinks-track" => true
    = csrf_meta_tags
  %body
    = render 'partials/header_bar'

    .wrapper
      - if notice.present?
        %p#notice= notice
      - if alert.present?
        %p#alert= alert

      = yield
